@page "/about"
@model Chirp.Razor.Pages.AboutMeModel
@{
    ViewData["Title"] = "Chirp!";
    Layout = "Shared/_Layout";
}

<div>
    <h2> About me </h2>
    <div>
    @if (Model.IsAuthenticated)
    {
            @foreach (var claim in Model.FilteredClaims)
            {
                <p>@claim</p>
            }
    }
    else
    {
            <p>You are not logged in.</p>
    }
    </div>
</div>
